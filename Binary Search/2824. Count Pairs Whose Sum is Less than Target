class Solution {
    public int countPairs(List<Integer> nums, int target) {
        int count = 0;
        Collections.sort(nums);
        int l=0, h=nums.size()-1;
        while(l<h){
            if(nums.get(l)+nums.get(h) < target){
                count += (h-l);
                l++;
            }
            else{
                h--;
            }
        }
        
        return count;
    }
}
