class Solution {
    public boolean check(int arr[], int key){
        int l=0;
        int h=arr.length-1;
        while(l<=h){
            int mid = (l+h)/2;
            if(arr[mid] == key){
                return true;
            }
            else if(arr[mid] < key){
                l = mid+1;
            }
            else{
                h = mid-1;
            }
        }
        return false;
    }
    public int getCommon(int[] nums1, int[] nums2) {
        for(int n: nums1){
            if(check(nums2, n)){
                return n;
            }
        }
        return -1;
    }
}
