class Solution {
    public int maxProfit(int[] prices) {
        int n = prices.length;
        int max=0;
        int mini=prices[0];
        for(int i=1; i<n; i++){
            max = Math.max(max, prices[i] - mini);
            mini = Math.min(mini, prices[i]);
        }
        return max;
    }
}
