class Solution {
    public int climbStairs(int n) {
        int dp[] = new int[n+1];
        Arrays.fill(dp, -1);
        return climb(dp, n);   
    } 
    public int climb(int dp[], int n) {
        if(n==0 || n==1) return 1;
        if(dp[n] != -1) return dp[n];

        int left =  climb(dp, n-1);
        int right =  climb(dp, n-2);
        dp[n] = left+right;
        return dp[n];
    } 
}
