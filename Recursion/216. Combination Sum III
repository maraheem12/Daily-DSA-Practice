class Solution {
    public void generate(int n, int k, int sum, int start,List<Integer> curr, List<List<Integer>> list){
        if(curr.size()==k){
            if(sum==n){
                list.add(new ArrayList<>(curr));
            }
            return;
        }

        for(int i=start; i<=9; i++){
            if(sum > n) break;
            curr.add(i);
            generate(n,k,sum+i,i+1,curr,list);
            curr.remove(curr.size()-1);
        }
    }
    public List<List<Integer>> combinationSum3(int k, int n) {
        List<List<Integer>> list = new ArrayList<>();
        generate(n,k,0,1,new ArrayList<>(),list);
        return list;
    }
}