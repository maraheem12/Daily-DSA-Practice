class Solution {
    public void backtrack(int nums[], boolean used[], List<Integer> curr, int n, List<List<Integer>> res){
        if(curr.size() == n){
            res.add(new ArrayList<>(curr));
            return;
        }

        for(int i=0; i<n; i++){
            if(used[i]) continue;
            if(i>0 && (nums[i] == nums[i-1]) && !used[i-1]) continue;
            curr.add(nums[i]);
            used[i] = true;
            backtrack(nums, used, curr, n, res);
            curr.remove(curr.size()-1);
            used[i] = false;
        }
    }
    public List<List<Integer>> permuteUnique(int[] nums) {
        int n = nums.length;
        List<List<Integer>> res = new ArrayList<>();
        Arrays.sort(nums);
        backtrack(nums, new boolean[n], new ArrayList<>(), n, res);
        return res;
    }
}
