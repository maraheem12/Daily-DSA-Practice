class Solution {
    public int prefixConnected(String[] words, int k) {
        HashMap<String, Integer> map = new HashMap<>();

        for(String word : words){
            if(word.length() >= k){
                String pre = word.substring(0, k);
                map.put(pre, map.getOrDefault(pre, 0) + 1);
            }
        }

        int res = 0;
        for(int count : map.values()){
            if(count >= 2) res++;
        }

        return res;
    }
}
