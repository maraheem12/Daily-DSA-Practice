class Solution {
    public String trimTrailingVowels(String s) {
        StringBuilder res = new StringBuilder();
        int n = s.length();
        int i=n-1;
        while(i>=0){
            char c = s.charAt(i);
            if(c=='a' || c=='e' || c=='i' || c=='o'|| c=='u'){
                i--;
            }
            else{
                break;
            }
        }
        for(int j=0; j<=i; j++){
            res.append(s.charAt(j));
        }
        return res.toString();
    }
}
